{
        "name": "next-js-cms",
        "buildCommand": "DISABLE_ESLINT_PLUGIN=true NEXT_DISABLE_ESLINT=true NEXT_DISABLE_TYPECHECK=true NODE_OPTIONS='--require ./next-env.js' next build --no-lint --no-mangling",
        "outputDirectory": ".next",
        "devCommand": "npm run dev",
        "installCommand": "npm install --legacy-peer-deps --force",
        "framework": "nextjs",
        "builds": [
                {
                        "src": "package.json",
                        "use": "@vercel/next"
                }
        ],
        "env": {
                "NODE_ENV": "production",
                "NEXT_TELEMETRY_DISABLED": "1",
                "NPM_FLAGS": "--legacy-peer-deps --force",
                "SKIP_LINTING": "true",
                "SKIP_TYPESCRIPT_CHECK": "true",
                "NEXT_DISABLE_ESLINT": "true",
                "NEXT_DISABLE_TYPECHECK": "true",
                "DISABLE_ESLINT_PLUGIN": "true",
                "DISABLE_TYPE_CHECKING": "true",
                "NEXT_PUBLIC_APP_URL": "https://next-js-cms.vercel.app"
        }
}